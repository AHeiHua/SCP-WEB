{
    "functions":{
        "api/app.py": {
            "runtime": "python3.7"
        }
    },
    "routes":[
        {
        "src": "templates/(.*)",
        "dest": "../templates/$1"
        },
        {
        "src": "/static/(.*)",
        "dest": "../static/$1"
        },
        {
        "src": "/(.*)",
        "dest": "/api/$1"
        }
    ]
}